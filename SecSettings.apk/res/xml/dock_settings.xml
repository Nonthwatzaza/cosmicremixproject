<?xml version="1.0" encoding="utf-8"?>
<PreferenceScreen android:title="StatusBar Mods" android:key="parent"
  xmlns:android="http://schemas.android.com/apk/res/android"
  xmlns:settings="http://schemas.android.com/apk/res/com.android.settings">
    <PreferenceCategory android:title="Statusbar Mods" android:key="bla" />
    <Preference android:title="Statusbar Tint (Xposed Module)" android:key="bla">
        <intent android:targetPackage="com.mohammadag.colouredstatusbar" android:action="android.intent.action.MAIN" android:targetClass="com.mohammadag.colouredstatusbar.MainActivity" />
    </Preference>
    <PreferenceScreen android:title="Statusbar Color Gradients" android:key="bla">
        <intent android:targetPackage="com.potato.gradientapp" android:action="android.intent.action.MAIN" android:targetClass="com.potato.gradientapp.MainActivity" />
    </PreferenceScreen>
    <PreferenceScreen android:title="Statusbar Clock Mods" android:key="bla">
        <intent android:targetPackage="com.potato.clock.main" android:action="android.intent.action.MAIN" android:targetClass="com.potato.clock.main.Main" />
    </PreferenceScreen>
    <PreferenceScreen android:title="Battery Styles" android:key="bla">
        <intent android:targetPackage="com.ghareeb.battery" android:action="android.intent.action.MAIN" android:targetClass="com.ghareeb.battery.Settings" />
    </PreferenceScreen>
    <PreferenceScreen android:title="Battery Bar" android:key="bla">
        <intent android:targetPackage="com.pvy.battbar.settings" android:action="android.intent.action.MAIN" android:targetClass="com.pvy.battbar.settings.options" />
    </PreferenceScreen>
    <PreferenceScreen android:title="Statusbar Brightness Bar Hide" android:key="bla" android:fragment="com.android.settings.RomControl" />
    <PreferenceCategory android:title="Extra Mods" android:key="bla" />
    <PreferenceScreen android:title="App Freeze" android:key="bla">
        <intent android:targetPackage="com.smartapp.appfreezer" android:action="android.intent.action.MAIN" android:targetClass="com.smartapp.appfreezer.MainActivity" />
    </PreferenceScreen>
    <PreferenceScreen android:title="Disable Per App Services" android:key="bla">
        <intent android:targetPackage="cn.wq.disableservice" android:action="android.intent.action.MAIN" android:targetClass="cn.wq.disableservice.MainActivity" />
    </PreferenceScreen>
    <PreferenceCategory android:title="dock" android:key="dock_settings" />
    <PreferenceScreen android:title="@string/dock_audio_settings_title" android:key="dock_audio" android:summary="@string/dock_settings_summary" />
    <CheckBoxPreference android:title="@string/dock_sounds_enable_title" android:key="dock_sounds" android:defaultValue="false" android:summaryOn="@string/dock_sounds_enable_summary_on" android:summaryOff="@string/dock_sounds_enable_summary_off" />
    <CheckBoxPreference android:persistent="false" android:title="@string/desk_cradle_title" android:key="cradle_enable" android:summary="@string/desk_cradle_summary" />
    <CheckBoxPreference android:persistent="false" android:title="@string/desk_home_screen_display" android:key="desk_home_screen_display" android:summary="@string/desk_home_screen_display_summary" />
    <PreferenceCategory android:title="@string/cover" android:key="cover" />
    <CheckBoxPreference android:persistent="false" android:title="@string/automatic_unlock" android:key="automatic_unlock" android:summary="@string/automatic_unlock_summary" />
    <CheckBoxPreference android:persistent="false" android:title="@string/automatic_receive_call" android:key="automatic_receive_call" android:summary="@string/automatic_receive_call_summary" />
    <PreferenceCategory android:title="@string/hdmi" android:key="hdmi" />
    <ListPreference android:entries="@array/audio_output_entries" android:title="@string/audio_output" android:key="audio_output" android:widgetLayout="@layout/round_more_icon" android:entryValues="@array/audio_output_values" />
    <PreferenceCategory android:title="@string/earset" android:key="earset" />
    <CheckBoxPreference android:persistent="false" android:title="@string/audio_applications" android:key="audio_applications" android:summary="@string/audio_applications_summary" />
</PreferenceScreen>